<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.foodorderapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/bon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/bon_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FoodOrderApp"
        android:usesCleartextTraffic="true">
        <activity android:name=".iletisimBilgileri"></activity>
        <activity android:name=".Hakkimizda" />
        <activity android:name=".AppIcon" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.FoodOrderApp.NoActionBar" />
        <activity android:name=".SepetOnayActivity" />
        <activity android:name=".SepetBosActivity" />
        <activity android:name=".YemekSepetActivity" />
        <activity android:name=".YemekDetaylarActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Giris">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>